{
	"name": "compliance-manager",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"bin": "./cli.ts",
	"scripts": {
		"dev": "vite dev --port 5173",
		"dev:api": "tsx --watch cli.ts --port 3000 --debug",
		"dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json && tsc --noEmit",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write 'src/**/*.{ts,js,svelte}' 'cli/**/*.ts'",
		"lint": "prettier --check 'src/**/*.{ts,js,svelte}' 'cli/**/*.ts' && eslint src cli",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run"
	},
	"dependencies": {
		"@types/ws": "^8.18.1",
		"commander": "^11.1.0",
		"cors": "^2.8.5",
		"express": "^4.21.2",
		"flowbite": "^2.5.2",
		"glob": "^10.4.5",
		"isomorphic-git": "^1.33.1",
		"js-yaml": "^4.1.0",
		"multer": "^2.0.2",
		"open": "^9.1.0",
		"ws": "^8.18.3",
		"xlsx": "^0.18.5",
		"yaml": "^2.8.1"
	},
	"devDependencies": {
		"@eslint/compat": "^1.3.2",
		"@eslint/js": "^9.34.0",
		"@sveltejs/adapter-static": "^3.0.9",
		"@sveltejs/kit": "^2.37.0",
		"@sveltejs/vite-plugin-svelte": "^6.1.4",
		"@tailwindcss/vite": "^4.1.13",
		"@types/cors": "^2.8.19",
		"@types/express": "^4.17.23",
		"@types/js-yaml": "^4.0.9",
		"@types/multer": "^2.0.0",
		"@vitest/browser": "^3.2.4",
		"carbon-icons-svelte": "^13.5.0",
		"carbon-preprocess-svelte": "^0.11.11",
		"concurrently": "^9.2.1",
		"eslint": "^9.34.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.12.1",
		"globals": "^16.3.0",
		"playwright": "^1.55.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.38.7",
		"svelte-check": "^4.3.1",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.13",
		"tsx": "^4.20.5",
		"typescript": "^5.9.2",
		"typescript-eslint": "^8.42.0",
		"vite": "^7.1.4",
		"vitest": "^3.2.4",
		"vitest-browser-svelte": "^0.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
