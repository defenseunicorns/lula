{
	"name": "compliance-manager",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"bin": "./cli.ts",
	"scripts": {
		"dev": "vite dev --port 5173",
		"dev:api": "tsx --watch cli.ts serve --dir ./examples/nist-800-53-rev4 --port 3000",
		"dev:full": "concurrently \"npm run dev:api\" \"npm run dev\"",
		"prebuild": "npm run update-frameworks",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run",
		"update-frameworks": "tsx scripts/fetch-cci-data.ts"
	},
	"dependencies": {
		"commander": "^11.1.0",
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"flowbite": "^2.2.0",
		"isomorphic-git": "^1.32.3",
		"lodash-es": "^4.17.21",
		"open": "^9.1.0",
		"xml2js": "^0.6.2",
		"yaml": "^2.3.4",
		"yauzl": "^3.1.0"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-auto": "^6.0.0",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/lodash-es": "^4.17.12",
		"@types/xml2js": "^0.4.14",
		"@types/yauzl": "^2.10.3",
		"@vitest/browser": "^3.2.3",
		"carbon-icons-svelte": "^13.5.0",
		"carbon-preprocess-svelte": "^0.11.11",
		"concurrently": "^9.2.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"playwright": "^1.53.0",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.0.0",
		"tsx": "^4.20.3",
		"typescript": "^5.5.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4",
		"vitest": "^3.2.3",
		"vitest-browser-svelte": "^0.1.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	}
}
